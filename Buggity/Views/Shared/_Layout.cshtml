<body>
    <header id="header" class="clearfix" data-ma-theme="blue">
        <ul class="h-inner">
            <li class="hi-trigger ma-trigger" data-ma-action="sidebar-open" data-ma-target="#sidebar">
                <div class="line-wrap">
                    <div class="line top"></div>
                    <div class="line center"></div>
                    <div class="line bottom"></div>
                </div>
            </li>

            <li class="hi-logo hidden-xs">
                <a href="/Home/Index" style="font-size:36px;font-family:'Tahoma'">Buggity</a>
            </li>



            @Html.Partial("_LoginPartial")
            <li class="pull-right">
                <ul class="hi-menu"></ul>
            </li>
        </ul>

        <!-- Top Search Content -->
        <div class="h-search-wrap">
            <div class="hsw-inner">
                <i class="hsw-close zmdi zmdi-arrow-left" data-ma-action="search-close"></i>
                <input type="text">
            </div>
        </div>
    </header>

    <section id="main">
        <aside id="sidebar" class="sidebar c-overflow mCustomScrollbar _mCS_1 mCS-autoHide" style="overflow: visible;">
            <div id="mCSB_1" class="mCustomScrollBox mCS-minimal-dark mCSB_vertical_horizontal mCSB_outside" style="max-height: none;" tabindex="0">
                <div id="mCSB_1_container" class="mCSB_container mCS_x_hidden mCS_no_scrollbar_x" style="position: relative; top: 23px; left: 0px; width: 100%;" dir="ltr">
                    <div class="s-profile">
                        <a href="" data-ma-action="profile-menu-toggle">
                            <div class="sp-pic">
                                
                            </div>

                            <div class="sp-info">
                                Alan Cravey

                                <i class="zmdi zmdi-caret-down"></i>
                            </div>
                        </a>

                        
                    </div>

                    <ul class="main-menu">
                        <li class="active">
                            <a> <i class="zmdi zmdi-home"></i> @Html.ActionLink("Home", "Index", "Home")</a>
                        </li>

                        @*@if (this.User.IsInRole(null))
                        {
                            
                        }*@

                        @if (this.User.IsInRole("Admin"))
                        {
                            <li>@Html.ActionLink("Manage User Roles", "ManageUserRoles", "Roles")</li>
                            <li>@Html.ActionLink("Tickets", "Index", "Tickets")</li>
                            <li>@Html.ActionLink("Create Project", "Create", "Projects")</li>
                            <li>@Html.ActionLink("View Projects", "Index", "Projects")</li>
                       }

                        @if (this.User.IsInRole("PM"))
                        {
                            <li>@Html.ActionLink("View Projects", "Index", "Projects")</li>
                            <li>@Html.ActionLink("View Tickets", "Index", "Tickets")</li>


                       }


                        @if (this.User.IsInRole("Developer"))

                        {
                            <li>@Html.ActionLink("View Tickets", "Index", "Tickets")</li>
                            <li>@Html.ActionLink("View Projects", "Index", "Projects")</li>

                        }

                        @if (this.User.IsInRole("Submitter"))

                        {
                            <li>@Html.ActionLink("Create Ticket", "Create", "Tickets")</li>
                            <li>@Html.ActionLink("View Tickets", "Index", "Tickets")</li>
                            <li>@Html.ActionLink("View Projects", "Index", "Projects")</li>

                        }



